{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "globals":{
    "angular": false,
    "define": false,
    "moment": false,
    "require": false,
    "module": false,
    "Map": false
  },
  "rules": {
    "array-bracket-spacing": 2,
    "block-scoped-var": 2,
    "brace-style": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "curly": [2, "multi-or-nest"],
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [2, 2],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "lines-around-comment": 2,
    "no-alert": 1,
    "no-array-constructor": 2,
    "no-debugger": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 2,
    "no-redeclare": 2,
    "no-spaced-func": 2,
    "no-shadow": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "all", "argsIgnorePattern": "^_"}],
    "operator-linebreak": [2, "before"],
    "quotes": [2,"single"],
    "quote-props": 2,
    "semi": [ 2, "always"],
    "semi-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "spaced-comment": 2,
    "wrap-regex": 2
  }
}
